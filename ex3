#include <stdio.h>
#include <string.h>

int main(){

    char str[50];
    char c1;
    char c2;
    int i;
    

    fgets(str,50,stdin);
    scanf("%c", &c1);
        getchar();
    scanf("%c", &c2);

    int tam = strlen(str);

    for(i=0;i<tam;i++){
        if(str[i]==c1){
            str[i]=c2;
            break;
        }
    }
    printf("%s",str);

    return 0;
}
